{
  "name": "ytt",
  "publisher": "fanda",
  "displayName": "yolo - YAPI TypeScript Generator",
  "description": "基于 YAPI 自动生成 TypeScript 接口请求工具",
  "version": "1.1.8",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other",
    "Snippets",
    "Programming Languages"
  ],
  "keywords": [
    "yapi",
    "typescript",
    "api",
    "generator",
    "interface",
    "yolo",
    "yolo-yapi",
    "fanda"
  ],
  "activationEvents": [
    "onCommand:ytt.startTerminal"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ytt.startTerminal",
        "title": "ytt",
        "category": "YAPI TypeScript Generator"
      }
    ],
    "configuration": {
      "title": "YAPI TypeScript Generator",
      "properties": {
        "ytt.yapiUrl": {
          "type": "string",
          "default": "",
          "description": "YAPI 服务器地址"
        },
        "ytt.username": {
          "type": "string",
          "default": "",
          "description": "YAPI 用户名"
        },
        "ytt.password": {
          "type": "string",
          "default": "",
          "description": "YAPI 密码"
        },
        "ytt.outputPath": {
          "type": "string",
          "default": "./src/api",
          "description": "生成的 TypeScript 文件输出路径"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "pinyin": "^4.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.32.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fanshihao123/vscode-YAPI-TypeScript-Generator.git"
  },
  "license": "MIT"
}
